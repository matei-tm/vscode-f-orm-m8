{
	"name": "flutter-sqlite-generator",
	"displayName": "Flutter Sqlite Generator",
	"description": "Vs Code Extension to generate Sqlite CRUD classes",
	"version": "0.0.1",
	"publisher": "Mircea-Tiberiu MATEI",
	"repository": {
        "type": "git",
        "url": "https://github.com/matei-tm/vscode-flutter-sqlite-extension.git"
    },
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.flutterSqliteGenerateFixture"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.flutterSqliteGenerateFixture",
				"title": "Flutter: Generate Sqlite Fixture"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.1.4",
		"vscode": "^1.1.25",
		"tslint": "^5.8.0",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42"
	}
}
